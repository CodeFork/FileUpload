{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },

  "Upload": {
    "Default": {
      "MaxLength": 500000,
      "SupportedExtensions": [ ".bundle", ".png", ".jpg", ".jpeg", ".gif" ],
      "StoragePath": "C:/Temp/Upload",
      "IsOverrideEnabled": true,
      "IsDownloadEnabled": true
    },
    "Profiles": {
      "xfv": {
        "MaxLength": 1000000,
        "SupportedExtensions": [ ".bundle" ],
        "StoragePath": "C:/Temp/Upload/Bundles",
        "IsOverrideEnabled": true
      }
    }
  }
}
